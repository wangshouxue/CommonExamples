<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MarqueeTextView">
        <attr name="orientation" format="integer" />
        <attr name="run_speed" format="integer" />
        <attr name="stay_time" format="integer" />
        <attr name="isMarquee" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="GradientColorButton">
        <attr name="text" format="string" />
        <attr name="isBold" format="boolean" />
        <attr name="textColor" format="integer" />
        <attr name="textsize" format="dimension"/>
        <attr name="borderWidth" format="dimension"/>
        <attr name="radius" format="dimension"/>
        <attr name="colorStart" format="integer" />
        <attr name="colorCenter" format="integer" />
        <attr name="colorEnd" format="integer" />
    </declare-styleable>
    <declare-styleable name="BubbleImageView">
        <attr name="triangle_marginTop" format="dimension" />
        <attr name="triangle_height" format="dimension" />
        <attr name="triangle_width" format="dimension" />
        <attr name="rect_radius" format="dimension" />
        <attr name="shadow_color" format="color" />
        <attr name="direction" format="integer">
            <enum name="left" value="1"></enum>
            <enum name="right" value="2"></enum>
        </attr>
    </declare-styleable>
</resources>